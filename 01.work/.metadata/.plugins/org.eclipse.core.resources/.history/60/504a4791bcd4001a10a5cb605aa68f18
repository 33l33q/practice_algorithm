package practice;

import java.util.Scanner;

public class If {
	
	public int If_test(){
		
		int avg = 0, temp = 0, sum = 0;
		
		Scanner scan = new Scanner(System.in);
		
		int arry[] = new int[5];
		
		for(int i = 0 ; i < 5 ; i++){
		
			arry[i] = scan.nextInt();
			
			if(arry[i] < 40){
				arry[i] = 40;
			}
			
			sum += arry[i];
			
		}
		
		avg = sum/5;
		
		System.out.println(avg);
		
		return avg;
	}
	
	public static void main(String[] args){
		
		If it = new If();
		
		int result = 0;
		
		result = it.If_test();
		
		System.out.println(result);

		
	}// end of main

}//end of class
