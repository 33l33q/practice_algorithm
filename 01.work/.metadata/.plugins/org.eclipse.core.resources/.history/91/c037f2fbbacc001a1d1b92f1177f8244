package ex;

public class ExamePratice {
	
	public static void main(String[] args){
		System.out.print("Hello World");
	
		int i = 0;
		int a = 0;
		
		for(i = 0;i <= 10; ++i ){
			a += i;
		}
		System.out.println(a);
	
		int j = 0;
		
		for(i = 1 ; i < 10 ; i++){
			for(j = 1 ; j < 10 ; j++){
				int x = i * j;
				System.out.println(i + "x" + j + "=" + x);
			}
		}
		
		double count = 0; 
		j = 3;
		for( i = 1 ; i < 10 ; i++){
			count = i * j ;
			if(!(count/4 = 3)){
				System.out.println(count);
			}
		}
		
	
	}

}
